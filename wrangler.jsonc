{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "cloudflare-template",
    "main": "worker.ts",
    "compatibility_date": "2025-04-22",
    "compatibility_flags": [
        "nodejs_compat",
        "global_fetch_strictly_public"
    ],
    "observability": {
        "logs": {
            "enabled": true
        }
    },
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "next-template",
            "database_id": "76508c42-476a-458d-ae33-045e333d8bf4",
            "migrations_dir": "migrations"
        }
    ],
    "ai": {
        "binding": "AI"
    },
    "r2_buckets": [
        {
            "bucket_name": "demo",
            "binding": "NEXT_INC_CACHE_R2_BUCKET"
        },
        {
            "bucket_name": "static",
            "binding": "static"
        }
    ],
    "durable_objects": {
        "bindings": [
            {
                "name": "NEXT_CACHE_DO_QUEUE",
                "class_name": "DOQueueHandler"
            },
            {
                "name": "NEXT_TAG_CACHE_DO_SHARDED",
                "class_name": "DOShardedTagCache"
            },
            {
                "name": "COUNTER_DO",
                "class_name": "Counter"
            }
        ]
    },
    "migrations": [
        {
            "tag": "v1",
            "new_sqlite_classes": [
                "DOQueueHandler",
                "DOShardedTagCache"
            ]
        },
        {
            "tag": "v2",
            "new_classes": [
                "Counter"
            ]
        }
    ],
    "services": [
        {
            "binding": "WORKER_SELF_REFERENCE",
            "service": "cloudflare-template"
        }
    ],
    // "triggers": {
    //     "crons": [
    //         "*/3 * * * *",
    //         "*/10 * * * *",
    //         "*/45 * * * *"
    //     ]
    // }
}